{
  "name": "key-defender",
  "version": "1.0.0",
  "main": "./bin/bundle.js",
  "author": {
    "name": "ouyang-chao",
    "email": "chao@ouyang.io"
  },
  "license": "Anti-996",
  "repository": "https://github.com/oychao/key-defender",
  "keywords": [
    "key",
    "list",
    "key-defender"
  ],
  "types": "./type.d.ts",
  "description": "Manage keys in React/Vue applications.",
  "files": [
    "LICENSE.md",
    "README.md",
    "type.d.ts",
    "bin/"
  ],
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
